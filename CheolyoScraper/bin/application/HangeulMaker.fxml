<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="115.0" prefWidth="593.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.HangeulController">
   <bottom>
      <Pane prefHeight="26.0" prefWidth="596.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="txtHangeul" layoutX="113.0" prefHeight="25.0" prefWidth="480.0" />
            <Button fx:id="btnHangeul" mnemonicParsing="false" onAction="#setHangeul" prefHeight="25.0" prefWidth="110.0" text="Set Hangeul" />
         </children>
      </Pane>
   </bottom>
   <left>
      <Pane prefHeight="71.0" prefWidth="593.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Current Page:" />
            <Button fx:id="btnCopyChar" layoutY="45.0" mnemonicParsing="false" onAction="#copyChar" prefHeight="25.0" prefWidth="109.0" text="Copy Character" />
            <Button fx:id="btnCopyWebpage" layoutX="470.0" layoutY="10.0" mnemonicParsing="false" onAction="#copyPage" prefHeight="25.0" prefWidth="109.0" text="Copy Page Site" />
            <Label fx:id="lblCurrentPage" layoutX="94.0" layoutY="12.0" prefHeight="21.0" prefWidth="72.0" />
         </children>
      </Pane>
   </left>
</BorderPane>
